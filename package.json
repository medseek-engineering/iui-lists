{
  "name": "iui-lists",
  "version": "1.0.1",
  "description": "Angular list directives",
  "main": "./lib/index.js",
  "private": false,
  "scripts": {
    "test": "node node_modules/karma/bin/karma start",
    "jshint": "node node_modules/jshint/bin/jshint -c config/.jshintrc app/modules test/modules app.js",
    "karma": "node node_modules/karma/bin/karma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medseek-engineering/iui-lists.git"
  },
  "author": "Influence Health, Inc.",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/medseek-engineering/iui-lists/issues"
  },
  "dependencies": {
    "gulp-bump": "^1.0.0",
    "lodash": "^2.4.1"
  },
  "devDependencies": {
    "browser-sync": "^2.8.2",
    "delayed-stream": "0.0.5",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "~2.1.7",
    "gulp-filter": "^3.0.0",
    "gulp-hologram": "^1.2.0",
    "gulp-html2js": "^0.2.0",
    "gulp-jshint": "^1.11.2",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-html2js": "^0.2.0",
    "gulp-rename": "~0.2.2",
    "gulp-uglify": "~0.2.0",
    "gulp-util": "~2.2.12",
    "iui-general": "git+ssh://git@github.com:medseek-engineering/iui-general.git",
    "jquery": "~2.1.4",
    "jshint": "~2.3.0",
    "jshint-stylish": "~1.0.0",
    "karma": "~0.12.1",
    "karma-chrome-launcher": "~0.1.2",
    "karma-coverage": "~0.1",
    "karma-firefox-launcher": "~0.1.3",
    "karma-html-reporter": "~0.1.1",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-jasmine": "~0.2.2",
    "karma-ng-html2js-preprocessor": "~0.1.0",
    "karma-requirejs": "~0.2.0",
    "karma-spec-reporter": "0.0.6",
    "ui-core": "git+ssh://git@github.com:medseek-engineering/ui-core.git",
    "medseek-config": "git+ssh://git@github.com:medseek-engineering/medseek-config.git"
  },
  "gulpSettings": {
    "base": "./app/",
    "styleModules": [
      "iui-general"
    ],
    "createTemplates": {
      "moduleName": "iui.listsTemplates",
      "prefix": "/$iui-lists/",
      "templateFile": "iui-list-templates.js"
    },
    "combineFiles": {
      "ignore": "!./lib/src/**/*.test.js"
    },
    "compass": {
      "sass": "lib/src",
      "css": "lib/dist",
      "require": []
    },
    "appFiles": {
      "styles": [
        "./lib/src/**/*.scss",
        "./lib/src/*.scss"
      ],
      "htmlFiles": [
        "./lib/src/**/*.html",
        "./lib/src/**/*.js"
      ],
      "jsFiles": [
        "./lib/src/**/*.js",
        "./lib/src/*.js"
      ],
      "templateFiles": [
        "./lib/src/**/*.html"
      ]
    },
    "destination": {
      "css": "./lib/dist",
      "js": "./lib/dist",
      "jsFile": "iui-lists.js",
      "jsFileMin": "iui-lists.min.js"
    }
  }
}
